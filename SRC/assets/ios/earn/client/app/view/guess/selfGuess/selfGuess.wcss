.body{
    height: 100%;
    padding: 20px 50px 167px;
    overflow: hidden auto;
    scroll-behavior: smooth;
}
.guess-detail{
    width:100%;
    height:160px;
    padding: 20px;
    margin-bottom: 50px;
    background:rgba(255,255,255,0.2);
    border-radius:6px;
    color: white;
    font-size: 24px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.guess-detail-item{
    display: flex;
    justify-content: space-between;
}
