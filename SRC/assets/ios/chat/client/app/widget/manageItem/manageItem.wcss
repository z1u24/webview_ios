.manage-ul{
    width:100%;
    padding:0;
    margin:0;
}
.manage-item-wrap{
    width: 650px;
    margin: 0 10px;
    padding: 0 20px;
    height: 120px;
    background: white;
    display: flex;
    align-items: center;
    border-bottom: solid #DBDBE5 1px;
}
.title{
    font-size: 28px;
    font-weight:bold;
    margin-left:0px;
    margin-right:auto;
}
.quantity{
    color: rgba(136, 136, 136, 0.87);
    font-size: 28px;
}
.goToImg{
    width:32px;
    height:32px;
    margin-left:10px;
    margin-right:0px;
}
