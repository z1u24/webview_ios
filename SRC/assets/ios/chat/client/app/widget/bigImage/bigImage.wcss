.modal-mask{
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 40px 75px 0;
    background-color: rgba(50, 50, 50, 1);
    position:absolute;
    width:100%;
    height:100%;
    top:0px;
    left:0px;
    overflow-x: hidden;
    overflow-y: auto;
}
.content{
    max-width: 100%;
}
