.refreshBtn{
    width: 48px;
    height: 48px;
    border: 24px solid transparent;
    position: absolute;
    right: 0;
}
.topBar{
    width: 100%;
}
.topBar-content{
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 88px;
}
.userHead{
    border: 2px solid #ffffff;  
    margin: 20px;
}
.total-asset{
    font-size: 32px;
    color: #fff;
}
.nextBtn{
    width: 48px;
    height: 48px;
    border: 24px solid transparent;
    position: absolute;
    right: 90px;
}