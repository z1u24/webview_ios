.util-list-wrap{
    width:100%;
    margin:0;
    background:#fff;
    box-shadow: 0px 0px 20px 0px rgba(0, 0, 0, 0.2);
    border-radius: 6px;
    overflow-x: hidden;
    overflow-y: auto;
}
.liItem{
    height:80px;
    display:flex;
    align-items:center;
    border-bottom: solid #CCCCCC 1px;
    margin:0;
}
.utilImg{
    width:36px;
    height:36px;
    margin-left:41px;
}
.utilText{
    font-size: 32px;
    margin-left:20px;
}